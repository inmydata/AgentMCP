INMYDATA_API_KEY=your_api_key_here
INMYDATA_TENANT=your_tenant_name
INMYDATA_CALENDAR=your_calendar_name
INMYDATA_USER=mcp-agent
INMYDATA_SESSION_ID=mcp-session

# OAuth / Auth Configuration
INMYDATA_MCP_HOST=mcp.inmydata.com
INMYDATA_SERVER=inmydata.com
INMYDATA_AUTH_SERVER=auth.inmydata.com

# Token Introspection Configuration (for PAT support)
# These credentials are used to validate Personal Access Tokens via introspection
INMYDATA_INTROSPECTION_CLIENT_ID=your_client_id_here
INMYDATA_INTROSPECTION_CLIENT_SECRET=your_client_secret_here

# Token Cache TTL (in seconds) - how long to cache introspected PAT results
# Default: 3600 seconds (60 minutes)
INMYDATA_TOKEN_CACHE_TTL=3600
